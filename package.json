  "description": "A monkey gui application",
  "main": "main.js",
  "build": {
    "appId": "com.monkeygui.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
